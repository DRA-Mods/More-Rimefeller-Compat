<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <Operation Class="VFECore.PatchOperationToggableSequence">
    <enabled>True</enabled>
    <mods>
      <li>Rimefeller</li>
      <li>Vanilla Factions Expanded - Ancients</li>
    </mods>
    <label>Pipeline Junction drainable by Rimefeller pipes:</label>
    <operations>
      <li Class="PatchOperationAdd">
        <xpath>/Defs/ThingDef[defName="VFEA_PipelineJunction"]/comps</xpath>
        <value>
          <li Class="Rimefeller.CompProperties_Pipe"/>
          <li>
            <compClass>VanillaExpandedRimefeller.JunctionToPipeNetComp</compClass>
          </li>
        </value>
      </li>
    </operations>
  </Operation>
  <Operation Class="VFECore.PatchOperationToggableSequence">
    <enabled>False</enabled>
    <label>Ancient Chemfuel Generator refuelable by Rimefeller pipes:</label>
    <mods>
      <li>Rimefeller</li>
      <li>Vanilla Factions Expanded - Ancients</li>
    </mods>
    <operations>
      <li Class="PatchOperationAdd">
        <xpath>/Defs/ThingDef[defName="VFEA_AncientChemfuelPoweredGenerator"]/comps</xpath>
        <value>
          <li Class="Rimefeller.CompProperties_Pipe"/>
        </value>
      </li>
    </operations>
  </Operation>
</Patch>