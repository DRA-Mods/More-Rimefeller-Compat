<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <Operation Class="VFECore.PatchOperationToggableSequence">
    <enabled>False</enabled>
    <mods>
      <li>Rimefeller</li>
      <li>Transport Cargo Pod</li>
    </mods>
    <label>Transport Cargo Pod refuelable by Rimefeller pipes:</label>
    <operations>
      <li Class="PatchOperationAdd">
        <xpath>/Defs/ThingDef[defName="Building_Helicopter"]/comps</xpath>
        <value>
          <li Class="Rimefeller.CompProperties_Pipe"/>
        </value>
      </li>
    </operations>
  </Operation>
</Patch>